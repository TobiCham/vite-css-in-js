{
	"name": "vite-css-in-js",
	"version": "1.3.0",
	"description": "Resolves css tagged templates to css files within Javascript",
	"author": "Tobi Chammings",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/TobiCham/vite-css-in-js.git"
	},
	"scripts": {
		"build": "rollup -c rollup.config.js"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist/index.js",
		"dist/index.mjs",
		"dist/index.d.ts"
	],
	"dependencies": {
		"@babel/core": ">7.0.0",
		"@babel/parser": "^7.0.0",
		"sha.js": "^2.4.11",
		"stylis": ">4.0.0"
	},
	"devDependencies": {
		"@types/babel__core": "^7.1.19",
		"@types/sha.js": "^2.4.0",
		"@types/stylis": "^4.0.2",
		"prettier": "^2.6.2",
		"rollup": "^2.72.1",
		"rollup-plugin-dts": "^4.2.1",
		"rollup-plugin-typescript2": "^0.31.2",
		"vite": ">2.0.0"
	},
	"keywords": [
		"vite", "plugin", "react", "vue", "css"
	]
}
