{
	"name": "vite-css-in-js",
	"version": "1.3.1",
	"description": "Resolves css tagged templates to css files within Javascript",
	"author": "Tobi Chammings",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/TobiCham/vite-css-in-js.git"
	},
	"scripts": {
		"build": "rollup -c rollup.config.mjs"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist/index.js",
		"dist/index.mjs",
		"dist/index.d.ts"
	],
	"dependencies": {
		"@babel/core": ">7.0.0",
		"@babel/parser": "^7.0.0",
		"sha.js": "^2.4.11",
		"stylis": ">4.0.0"
	},
	"devDependencies": {
		"@types/babel__core": "^7.20.0",
		"@types/sha.js": "^2.4.0",
		"@types/stylis": "^4.0.2",
		"prettier": "^2.8.8",
		"rollup": "^3.21.0",
		"rollup-plugin-dts": "^5.3.0",
		"rollup-plugin-typescript2": "^0.34.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.1"
	},
	"keywords": [
		"vite",
		"plugin",
		"react",
		"vue",
		"css"
	]
}
